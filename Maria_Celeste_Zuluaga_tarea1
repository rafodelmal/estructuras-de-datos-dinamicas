import sys

def sumar(n1, n2):
    n1 = int(input("Ingrese primer número: "))
    n2 = int(input("Ingrese segundo número: "))
    maxi = sys.maxsize
    r1 = n1 + maxi
    r2 = n2 + maxi
    return r1, r2

res1, res2 = sumar(0,0) #Llamar función y guardar resultados.


print("La suma del primer valor a su máximo es: ", res1)
print("La suma del segundo valor a su máximo es: ", res2)